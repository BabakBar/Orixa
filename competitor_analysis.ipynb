    "            function_name = site[\"name\"]\n",
    "            content = site[\"content\"]\n",
    "            extracted_content = extract(content)\n",
    "\n",
    "            data[\"Site\"].append(function_name)\n",
    "            data[\"Provider Name\"].append(provider_name)\n",
    "\n",
    "            extracted_json = json.loads(extracted_content)\n",
    "            data[\"Cheapest Plan\"].append(extracted_json[\"cheapest\"][\"name\"])\n",
